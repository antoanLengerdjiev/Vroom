@model string

<td class="text-right">
    <div class="btn-group" role="group">
        <a type="button" class="btn btn-primary" asp-action="Edit"  asp-route-id="@Model"><i class="fa-regular fa-pen-to-square"></i></a>
        <form method="post" action="Delete">
            <button type="submit" onclick="if (!confirm('are you sure you want to delete this Item')){ return false; }" class="btn btn-danger rounded-0" asp-action="Delete" asp-route-id="@Model"><i class="fa-regular fa-trash-can"></i></button>
        </form>
    </div>
</td>